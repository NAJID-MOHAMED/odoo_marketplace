<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Vendor Sequence -->
        <record id="sequence_marketplace_vendor" model="ir.sequence">
            <field name="name">Marketplace Vendor Sequence</field>
            <field name="code">marketplace.vendor</field>
            <field name="prefix">VEN</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Product Sequence -->
        <record id="sequence_marketplace_product" model="ir.sequence">
            <field name="name">Marketplace Product Sequence</field>
            <field name="code">marketplace.product</field>
            <field name="prefix">PRD</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Order Sequence -->
        <record id="sequence_marketplace_order" model="ir.sequence">
            <field name="name">Marketplace Order Sequence</field>
            <field name="code">marketplace.order</field>
            <field name="prefix">ORD/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Commission Sequence -->
        <record id="sequence_marketplace_commission" model="ir.sequence">
            <field name="name">Marketplace Commission Sequence</field>
            <field name="code">marketplace.commission</field>
            <field name="prefix">COM</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Payout Sequence -->
        <record id="sequence_marketplace_payout" model="ir.sequence">
            <field name="name">Marketplace Payout Sequence</field>
            <field name="code">marketplace.payout</field>
            <field name="prefix">PAY/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
        </record>

    </data>
</odoo>